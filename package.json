{
  "name": "WRMC-Website",
  "version": "0.1.0",
  "private": true,
  "contributors": [
    "Ethan Berner <eberner@middlebury.edu>",
    "Sam Chester <schester@middlebury.edu>",
    "Matthew Durst <madurst@middlebury.edu>",
    "Ethan Saxenian <esaxenian@middelbury.edu>",
    "Madeleine Van Beek <mvanbeek@middlebury.edu>",
    "Yichen Yang <yicheny@middlebury.edu> <cliffyyc@outlook.com>"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "jest",
    "lint": "eslint src"
  },
  "dependencies": {
    "next": "10.0.8",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.21.0",
    "eslint-plugin-react": "^7.22.0",
    "fetch-mock-jest": "^1.5.1",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md,json,jsx}": "prettier --write"
  }
}
